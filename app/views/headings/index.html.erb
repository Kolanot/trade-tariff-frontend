<div class='browse-sections core-content group'>
  <strong>Section</strong>: <%= link_to @section, section_chapters_path(@section) %>
  <br/>
  <strong>Chapter</strong>: <%= link_to @chapter, section_chapter_commodities_path(@section, @chapter) %>
  <br/>
  <h2>Headings in this chapter</h2>

  <table>
    <thead>
      <tr>
        <td colspan="5">
          <strong>No</strong>
        </td>
      </tr>
      <tr>
        <td>
          <strong>Description</strong>
        </td>
        <td>
          <strong>Footnotes</strong>
        </td>
        <td>
          <strong>Your Notes</strong>
        </td>
      </tr>
    </thead>
    <tbody>
      <%= render @headings %>
    </tbody>
  </table>
</div>
