<article class="tariff commodity">
  <div class="inner">
    <nav>
      <ul>
        <li>
          <strong>Section</strong>:
          <%= link_to @section, section_chapters_path(@section) %>
        </li>
        <li>
          <strong>Chapter</strong>:
          <%= link_to @chapter, section_chapter_headings_path(@section, @chapter) %>
        </li>
        <li>
          <strong>Commodities in</strong>:
          <%= link_to @heading, section_chapter_heading_commodities_path(@section, @chapter, @heading) %>
        </li>
      </ul>
    </nav>

    <h2>Commodities in this heading</h2>

    <table>
      <thead>
        <tr>
          <td>
            <strong>Code</strong>
          </td>
          <td>
            <strong>Description</strong>
          </td>
          <td>
            <strong>Basic duty rate</strong>
          </td>
          <td>
            <strong>Footnotes</strong>
          </td>
          <td>
            <strong>Your Notes</strong>
          </td>
        </tr>
      </thead>
      <tbody>
        <%= render @commodities %>
      </tbody>
    </table>
  </div>
</article>
